# zadanie 1

#a
a=cotd(5/12);
disp(a);

#b
b=[3 -2 3; -2 3 8; 6 4 0];
disp(inv(b));

#c
w=25;
I=25;
E = (I*w^2)/2;

disp(E);

#d
sum = 0;
for n = -3:50
    sum = sum + (n+4) / (n^2+2);
end
disp(sum);




# zadanie 3
data = [4 2 3; 2 0 1; 3 4 0];

labels = ["Złote", "Srebrne", "Brązowe"];
years = ["2010", "2015", "2020"];

b = bar(data', group = years);

legend(["2010", "2015", "2020"]);

title("Liczba medali w poszczególnych latach");
xlabel("Typ medalu");
ylabel("Liczba medali");


